{
  "types": {
    "URL": ["urlData", "utmSource", "utmMedium", "utmCampaign"],
    "Payment": ["payMode", "payUser", "payLink", "payAmount", "payNote"],
    "WiFi": ["wifiSsid", "wifiPwd", "wifiSec", "wifiHidden"],
    "Contact": [
      "vFirst",
      "vLast",
      "vOrg",
      "vTitle",
      "vPhone1",
      "vPhone1Type",
      "vPhone2",
      "vPhone2Type",
      "vEmail1",
      "vEmail1Type",
      "vEmail2",
      "vEmail2Type",
      "vWebsite",
      "vBday",
      "vStreet",
      "vCity",
      "vRegion",
      "vPostal",
      "vCountry",
      "vNote"
    ],
    "Message": ["msgMode", "smsNumber", "smsText"],
